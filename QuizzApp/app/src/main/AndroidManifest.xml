<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wil.quizzapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_quizzapp_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_quizzapp_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizzApp">
        <activity
            android:name=".ResultHistoryActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>/>
        <activity
            android:name=".NewQuestionActivity"
            android:exported="false" />
        <activity
            android:name=".QuestionListActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>
        <activity
            android:name=".NewPlayer"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>